1. SELECT 
   
FROM
    trips t
JOIN
    weather w
ON
    date_trip=w.Date
GROUP BY 1, 2, 3
ORDER BY 2 DESC
LIMIT 3

2. SELECT 
	station_id,
	COUNT(*) 
	
	
FROM 
	Status 
	
WHERE 
	bikes_available = 0

GROUP BY station_id	

ORDER BY 
	COUNT(*) DESC
	
LIMIT 100
3. SELECT 
	station.name 
	station.dockcount
	station.station_id
	count(stations.station_id)
	trips.start_station
	
FROM 
	stations 
LEFT JOIN
	trips
ON 
	s.station_id = t.start_station

Order by s.dockcount desc
